<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.shoppingmall.admin.mapper.AdminMapper">
    <select id="adminLogin" resultType="com.example.shoppingmall.admin.domain.Admin" >
        select admin_id, admin_pw
        from admin
        where admin_id = #{admin_id}
          and admin_id = #{admin_pw}
    </select>
</mapper>